<% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error) { %>
<div class="d-flex align-items-center justify-content-between alert alert-warning alert-dismissible fade show" role="alert">
    <%= error.msg %>
    <button type="button" class="btn-close" data-dismiss="alert" aria-label="Close">
    </button>
</div>
<% }); %> <% } %> <% if(success_msg != ''){ %>
<div class="d-flex align-items-center justify-content-between alert alert-success alert-dismissible fade show" role="alert">
    <%= success_msg %>
    <button type="button" class="btn-close" data-dismiss="alert" aria-label="Close">
    </button>
</div>
<% } %> <% if(error_msg != ''){ %>
<div class="d-flex align-items-center justify-content-between alert alert-danger alert-dismissible fade show" role="alert">
    <%= error_msg %>
    <button type="button" class="btn-close" data-dismiss="alert" aria-label="Close">
    </button>
</div>
<% } %> <% if(error != ''){ %>
<div class="d-flex align-items-center justify-content-between alert alert-danger alert-dismissible fade show" role="alert">
    <%= error %>
    <button type="button" class="btn-close" data-dismiss="alert" aria-label="Close">
    </button>
</div>
<% } %>