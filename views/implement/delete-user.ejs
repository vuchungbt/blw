<!-- Modal 1 My profile-->
<div class="modal fade" id="deleteUserModal" tabindex="-1" role="dialog" aria-labelledby="deleteUserModal"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Delete Account</h5>
                <button type="button" class="btn-close" data-dismiss="modal" aria-label="Close">
                </button>
            </div>
            <div class="modal-body">
                <form id="addmember" action="/home/deleteuser" method="post">
                    <h3 class="text-danger">Are you sure?</h3>
                    <div class="form-group">
                        <label for="name" class="col-form-label">Name:</label>
                        <input type="text" class="form-control" id="name" disabled="" name="name" value="<%= typeof name != 'undefined' ? name : '' %>">
                    </div>
                    <div class="form-group">
                        <label for="email" class="col-form-label">Email:</label>
                        <input type="text" class="form-control" id="email" disabled="" name="email" value="<%= typeof email != 'undefined' ? email : '' %>">
                    </div>
                    <input type="hidden" class="form-control" id="id" name="id" value="<%= typeof id != 'undefined' ? id : '' %>">
                    
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
                </form>
            </div>
        </div>
    </div>
</div>